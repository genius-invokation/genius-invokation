{
  "compilerOptions": {
    "composite": true,
    "target": "ES2023",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],

    "jsx": "preserve",
