{
  "name": "@gi-tcg/custom-data-loader",
  "version": "0.17.4+gi-6-0-0",
  "description": "Provides an easy way to load your custom data for GI-TCG",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "bun": "./src/index.ts",
      "development": "./src/index.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "check": "tsc --noEmit",
    "build:runtime_types": "dts-bundle-generator --config=dts-bundle-generator.config.json",
    "build:ts": "tsup",
    "build": "bun run build:ts && bun run build:runtime_types"
  },
  "dependencies": {
    "@gi-tcg/data": "workspace:*"
  },
  "peerDependencies": {
    "@gi-tcg/core": "workspace:*"
  },
  "devDependencies": {
    "@gi-tcg/assets-manager": "workspace:*",
    "@gi-tcg/deck-builder": "workspace:*",
    "@gi-tcg/web-ui-core": "workspace:*",
    "@gi-tcg/typings": "workspace:*",
    "dts-bundle-generator": "^9.5.1",
    "monaco-editor": "^0.52.2",
    "solid-js": "catalog:",
    "vite": "catalog:",
    "vite-plugin-solid": "catalog:"
  }
}
