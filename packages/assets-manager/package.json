{
  "name": "@gi-tcg/assets-manager",
  "version": "0.17.4",
  "type": "module",
  "description": "Manage assets of GI-TCG for Web app",
  "dependencies": {
    "@gi-tcg/typings": "workspace:*",
    "p-limit": "^7.1.1"
  },
  "devDependencies": {
    "@gi-tcg/config": "workspace:*",
    "@gi-tcg/core": "workspace:*"
  },
  "main": "./dist/index.js",
  "imports": {
    "#src/*": "./src/*.ts"
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./data/*": {
      "bun": "./src/data/*.json",
      "import": "./dist/*.json"
    }
  },
  "scripts": {
    "build:data": "bun --preload=./scripts/fetch_preload.ts ./scripts/fetch.ts",
    "build:ts": "tsup",
    "build": "bun run build:data && bun run build:ts"
  }
}
